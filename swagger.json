{
    "swagger": "2.0",
    "info": {
      "title": "Event Management API",
      "description": "API for managing events and attendees",
      "version": "1.0.0"
    },
    "host": "cse341-finalproject-87ca.onrender.com",
    "basePath": "/",
    "schemes": [
      "https"
    ],
    "paths": {
      "/": {
        "get": {
          "tags": [
            "Hello World"
          ],
          "description": "Returns a welcome message",
          "responses": {
            "200": {
              "description": "SUCCESS"
            }
          }
        }
      },
      "/events": {
        "get": {
          "tags": ["Events"],
          "summary": "Gets all events",
          "responses": {
            "200": {
              "description": "SUCCESS"
            }
          }
        },
        "post": {
          "tags": ["Events"],
          "summary": "Creates a new event",
          "parameters": [
            {
              "name": "body",
              "in": "body",
              "schema": {
                "type": "object",
                "properties": {
                  "title": {
                    "type": "string",
                    "example": "Event Name"
                  },
                  "description": {
                    "type": "string",
                    "example": "Event Description"
                  },
                  "date": {
                    "type": "string",
                    "format": "date",
                    "example": "2024-01-01"
                  },
                  "location": {
                    "type": "string",
                    "example": "Event Location"
                  },
                  "created_by": {
                    "type": "string",
                    "example": "Joe Admin"
                  }
                },
                "required": ["title", "description", "location", "date", "created_by"]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "SUCCESS - Event Created"
            },
            "401": {
              "description": "Authentication Error"
            },
            "412": {
              "description": "Validation Error"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/events/{id}": {
        "get": {
          "tags": ["Events"],
          "summary": "Gets a single event by ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "SUCCESS"
            },
            "404": {
              "description": "Event Not Found"
            }
          }
        },
        "put": {
          "tags": ["Events"],
          "summary": "Updates an event by ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            },
            {
              "name": "body",
              "in": "body",
              "schema": {
                "type": "object",
                "properties": {
                  "title": {
                    "type": "string",
                    "example": "Updated Event Name"
                  },
                  "description": {
                    "type": "string",
                    "example": "Updated Event Description"
                  },
                  "date": {
                    "type": "string",
                    "format": "date",
                    "example": "2024-01-02"
                  },
                  "location": {
                    "type": "string",
                    "example": "Updated Event Location"
                  },
                  "created_by": {
                    "type": "string",
                    "example": "Updated Creator/Editor"
                  }
                },
                "required": ["name", "date", "location", "description", "created_by"]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "SUCCESS - Event Updated"
            },
            "401": {
              "description": "Authentication Error"
            },
            "404": {
              "description": "Event Not Found"
            },
            "412": {
              "description": "Validation Error"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        },
        "delete": {
          "tags": ["Events"],
          "summary": "Deletes an event by ID",
          "parameters": [
            {
              "name": "Event ID",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "SUCCESS - Event Deleted"
            },
            "401": {
              "description": "Authentication Error"
            },
            "404": {
              "description": "Event Not Found"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/participants": {
        "get": {
          "tags": ["Participants"],
          "summary": "Gets all participants",
          "responses": {
            "200": {
              "description": "SUCCESS"
            }
          }
        },
        "post": {
          "tags": ["Participants"],
          "summary": "Creates a new participant",
          "parameters": [
            {
              "name": "body",
              "in": "body",
              "schema": {
                "type": "object",
                "properties": {
                  "first_name": {
                    "type": "string",
                    "example": "Participant First Name"
                  },
                  "last_name": {
                    "type": "string",
                    "example": "Participant Last Name"
                  },
                  "email": {
                    "type": "email",
                    "example": "participant@email.com"
                  },
                  "event_id": {
                  "type": "string",
                  "example": "Event ID"
                  },
                  "joined_at": {
                    "type": "string",
                    "format": "date",
                    "example": "2024-01-01"
                  },
                  "status": {
                    "type": "string",
                    "example": "Joinend, Pending, Paid, etc."
                  }
                },
                "required": ["first_name", "last_name", "email", "joined_at", "status"]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "SUCCESS - Participant Created"
            },
            "401": {
              "description": "Authentication Error"
            },
            "412": {
              "description": "Validation Error"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/participants/{id}": {
        "get": {
          "tags": ["Participants"],
          "summary": "Gets a single participant by ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "SUCCESS"
            },
            "404": {
              "description": "Participant Not Found"
            }
          }
        },
        "put": {
          "tags": ["Participants"],
          "summary": "Updates a participant by ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            },
            {
              "name": "body",
              "in": "body",
              "schema": {
                "type": "object",
                "properties": {
                  "first_name": {
                    "type": "string",
                    "example": "Updated First Name"
                  },
                  "last_name": {
                    "type": "string",
                    "example": "Updated Last Name"
                  },
                  "email": {
                    "type": "email",
                    "example": "Updated Email Address"
                  },
                  "joined_at": {
                    "type": "string",
                    "format": "date",
                    "example": "2024-01-02"
                  },
                  "status": {
                    "type": "string",
                    "example": "Updated: Joinend, Pending, Paid, etc."
                  }
                },
                "required": ["first_name", "last_name", "email", "joined_at", "status"]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "SUCCESS - Participant Updated"
            },
            "401": {
              "description": "Authentication Error"
            },
            "404": {
              "description": "Event Not Found"
            },
            "412": {
              "description": "Validation Error"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        },
        "delete": {
          "tags": ["Participants"],
          "summary": "Deletes a participant by ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "SUCCESS - Participant Deleted"
            },
            "401": {
              "description": "Authentication Error"
            },
            "404": {
              "description": "Event Not Found"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      }
    }
  }
